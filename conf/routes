# Routes
# This file defines all application routes (Higher priority routes first)
# ~~~~

GET     /                           controllers.StaticPageController.show(name="index")
GET     /anime                      controllers.StaticPageController.show(name="anime")
GET     /program                    controllers.StaticPageController.show(name="program")
GET     /light_novel                controllers.StaticPageController.show(name="light_novel")
GET     /link                       controllers.StaticPageController.show(name="link")
GET     /site_info                  controllers.StaticPageController.show(name="site_info")

GET     /assets/*file               controllers.Assets.versioned(path="/public", file: Asset)
